{
  "name": "trak-cli",
  "version": "0.2.0",
  "description": "AI-native task tracker for agent swarms. SQLite speed, JSONL git sync, multi-agent coordination.",
  "main": "dist/cli.js",
  "bin": {
    "trak": "./dist/cli.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "./node_modules/.bin/tsc && ./node_modules/.bin/esbuild src/cli.ts --bundle --platform=node --outfile=dist/cli.bundle.js --external:better-sqlite3 --format=cjs --minify 2>/dev/null",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "bench": "node benchmarks/bench.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ai",
    "agent",
    "task-tracker",
    "cli",
    "sqlite",
    "swarm",
    "claude",
    "cursor",
    "codex",
    "multi-agent"
  ],
  "author": "Kevin Keeland",
  "license": "MIT",
  "homepage": "https://github.com/kkeeland/trak",
  "repository": {
    "type": "git",
    "url": "https://github.com/kkeeland/trak.git"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.19.7",
    "esbuild": "^0.27.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
